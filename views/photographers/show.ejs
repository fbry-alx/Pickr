<h2 class="page-header"><%= photographer.name %></h2>
<div class="btn-row">
  <a class="btn btn-primary" href="/photographers/<%= photographer.id %>/edit">Edit</a>
  <%- include('../partials/deleteForm',
              { url: `/photographers/${photographer.id}` }) %>
</div>
<% if (photosByPhotographer.length > 0) { %>
<h2 class="page-header">Photos By Photographer</h2>
<%- include('../partials/photoGrid', { photos: photosByPhotographer }) %>
<% } %>